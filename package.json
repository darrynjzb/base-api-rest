    "mask-json": "^1.0.3",
    "mocha": "^9.0.2",
    "mockery": "^2.1.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.11",
    "nyc": "^15.1.0",
    "socket.io": "^4.1.2"
